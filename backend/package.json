{
  "name": "ref-nearchaintools--backend",
  "version": "0.0.1",
  "description": "REF finance community and statistics website - backend",
  "author": "Miroslav Å meral",
  "license": "MIT",
  "keywords": [
    "Blockchain",
    "NEAR",
    "Ref finance",
    "Prisma Client",
    "Typescript",
    "NestJs"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "console:dev": "ts-node -r tsconfig-paths/register src/console-apps/console-scripts.ts",
    "standalone:dev": "ts-node -r tsconfig-paths/register src/standalone.ts",
    "console": "node dist/console-apps/console-scripts",
    "standalone": "node dist/standalone",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d --build",
    "docker:studio": "docker-compose -f docker-compose.studio.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@apollo/gateway": "^0.38.1",
    "@devoxa/prisma-relay-cursor-connection": "1.1.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@prisma/client": "^2.28.0",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "5.0.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "commander": "^7.2.0",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "discord.js": "^13.1.0",
    "graphql": "15.5.0",
    "graphql-subscriptions": "1.2.1",
    "graphql-tools": "7.0.4",
    "lodash": "^4.17.21",
    "near-api-js": "^0.42.0",
    "near-cli": "^2.1.1",
    "os": "^0.1.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "4.1.6",
    "ts-morph": "^11.0.3",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/testing": "7.6.15",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.1",
    "@types/cron": "^1.7.2",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.19",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "chance": "1.1.7",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "^2.30.0",
    "prisma-dbml-generator": "0.5.0",
    "rimraf": "3.0.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      ".+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
